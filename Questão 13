#include <stdio.h>
#include <stdlib.h>

int main(){
    
    int a, b;
    
    printf("Digite um número inteiro para A: ");
    scanf("%d", &a);
    
    printf("Digite um número inteiro para B: ");
    scanf("%d", &b);
    
    printf("\nINVERSÃO DE VALORES:");
    printf("\nAgora a variável A possui o valor %d", b);
    printf("\nA variável B possui o valor %d", a);

    return 0;
}
